{
  "schemaVersion": 1,
  "id": "acer",
  "version": "${version}",
  "name": "Acer",
  "description": "This is an example description! Tell everyone what your mod is about!",
  "authors": [
    "Rodrigo Addor <me@rodr.dev>"
  ],
  "contact": {
    "homepage": "https://github.com/rodrigoaddor/acer",
    "sources": "https://github.com/rodrigoaddor/acer"
  },
  "license": "CC0-1.0",
  "icon": "assets/acer/icon.png",
  "environment": "server",
  "entrypoints": {
    "server": [
      {
        "adapter": "kotlin",
        "value": "dev.rodr.acer.AcerMod"
      }
    ]
  },
  "mixins": [
    "acer.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.17.0",
    "fabric-api": "*",
    "fabric-language-kotlin": "*",
    "minecraft": "1.21.x",
    "fabric-permissions-api-v0": "*"
  }
}
